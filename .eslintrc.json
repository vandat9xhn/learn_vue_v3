{
    "root": true,
    "env": {
        "node": true
    },
    "extends": ["plugin:vue/vue3-essential", "eslint:recommended"],
    "parserOptions": {
        "parser": "babel-eslint"
    },
    "rules": {
        "semi": 1,
        "quotes": [1, "single"],
        "arrow-spacing": [1, { "before": true, "after": true }],
        "camelcase": 0,
        "no-use-before-define": 0,
        "no-console": 0,
        "object-curly-newline": 0,
        "no-unused-vars": "off",
        "vue/no-reserved-keys": "off"
    }
}
